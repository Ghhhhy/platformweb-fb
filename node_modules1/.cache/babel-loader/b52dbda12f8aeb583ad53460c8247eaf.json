{\r\n    window.removeEventListener('resize', resize)\r\n  })\r\n\r\n  watch(option, () => {\r\n    setOption(option)\r\n  }, { deep: true, immediate: true })\r\n\r\n  return {\r\n    chartIdLeft,\r\n    setOption,\r\n    getChartInstance,\r\n    resize\r\n  }\r\n}\r\nexport const useChartRight = (option = {}) => {\r\n  const initOptions = ref({\r\n    backgroundColor: getColor('white'),\r\n    color: getColor(['blue', 'green']),\r\n    title: getTitle(),\r\n    // 指示器\r\n    tooltip: {\r\n      ...getTooltip(),\r\n      formatter: getTooltipFormatter()\r\n    },\r\n    // 位置\r\n    grid: getGrid({ left: 16, top: 90, right: 16, bottom: 10 }),\r\n    // 图例\r\n    legend: getLegend(),\r\n    // x轴\r\n    xAxis: {\r\n      ...getAxis({ type: 'category', splitLineShow: true }),\r\n      axisLabel: getAxisLabel()\r\n    },\r\n    // y轴\r\n    yAxis: {\r\n      ...getAxis({ splitLineShow: true }),\r\n      axisLabel: getAxisLabel()\r\n    },\r\n    series: []\r\n  })\r\n\r\n  const getRandom = (digit = 6) => {\r\n    return String(Math.random()).split('.')[1].substring(2, digit + 2)\r\n  }\r\n  const chartIdRight = `chart${getRandom()}-${++uniqueKey}`\r\n\r\n  let chartInstance = null\r\n\r\n  // 初始化\r\n  const init = () => {\r\n    console.log(chartIdRight, document.getElementById(chartIdRight))\r\n    chartInstance = echarts.init(\r\n      document.getElementById(chartIdRight),\r\n      null,\r\n      {\r\n        // 渲染成svg 支持放大清晰度不变\r\n        renderer: 'svg'\r\n      }\r\n    )\r\n    setOption(initOptions)\r\n  }\r\n\r\n  const getChartInstance = () => {\r\n    return chartInstance\r\n  }\r\n\r\n  const setOption = (option) => {\r\n    chartInstance?.setOption(unref(option))\r\n  }\r\n\r\n  function resize() {\r\n    chartInstance?.resize?.()\r\n  }\r\n\r\n  onMounted(() => {\r\n    init()\r\n    window.addEventListener('resize', resize)\r\n  })\r\n\r\n  onBeforeUnmount(() => {\r\n    window.removeEventListener('resize', resize)\r\n  })\r\n\r\n  watch(option, () => {\r\n    setOption(option)\r\n  }, { deep: true, immediate: true })\r\n\r\n  return {\r\n    chartIdRight,\r\n    setOption,\r\n    getChartInstance,\r\n    resize\r\n  }\r\n}\r\n"],"mappings":";;;;;;AAAA,SAASA,KAAK,EAAEC,SAAS,EAAEC,eAAe,EAAEC,KAAK,EAAEC,GAAG,QAAQ,sBAAsB;AACpF,OAAO,KAAKC,OAAO,MAAM,SAAS;AAClC,SACEC,OAAO,EAAEC,YAAY,EACrBC,QAAQ,EAAEC,OAAO,EAAEC,SAAS,EAC5BC,QAAQ,EACRC,UAAU,EACVC,mBAAmB,QACd,yDAAyD;AAEhE,IAAIC,SAAS,GAAG,CAAC;AAEjB,OAAO,IAAMC,YAAY,GAAG,SAAfA,YAAYA,CAAA,EAAoB;EAAA,IAAhBC,MAAM,GAAAC,SAAA,CAAAC,MAAA,QAAAD,SAAA,QAAAE,SAAA,GAAAF,SAAA,MAAG,CAAC,CAAC;EACtC,IAAMG,WAAW,GAAGhB,GAAG,CAAC;IACtBiB,eAAe,EAAEb,QAAQ,CAAC,OAAO,CAAC;IAClCc,KAAK,EAAEd,QAAQ,CAAC,CAAC,MAAM,EAAE,OAAO,CAAC,CAAC;IAClCe,KAAK,EAAEZ,QAAQ,CAAC,CAAC;IACjB;IACAa,OAAO,EAAAC,aAAA,CAAAA,aAAA,KACFb,UAAU,CAAC,CAAC;MACfc,SAAS,EAAEb,mBAAmB,CAAC;IAAC,EACjC;IACD;IACAc,IAAI,EAAElB,OAAO,CAAC;MAAEmB,IAAI,EAAE,EAAE;MAAEC,GAAG,EAAE,EAAE;MAAEC,KAAK,EAAE,EAAE;MAAEC,MAAM,EAAE;IAAG,CAAC,CAAC;IAC3D;IACAC,MAAM,EAAEtB,SAAS,CAAC,CAAC;IACnB;IACAuB,KAAK,EAAAR,aAAA,CAAAA,aAAA,KACAnB,OAAO,CAAC;MAAE4B,IAAI,EAAE,UAAU;MAAEC,aAAa,EAAE;IAAK,CAAC,CAAC;MACrDC,SAAS,EAAE7B,YAAY,CAAC;IAAC,EAC1B;IACD;IACA8B,KAAK,EAAAZ,aAAA,CAAAA,aAAA,KACAnB,OAAO,CAAC;MAAE6B,aAAa,EAAE;IAAK,CAAC,CAAC;MACnCC,SAAS,EAAE7B,YAAY,CAAC;IAAC,EAC1B;IACD+B,MAAM,EAAE;EACV,CAAC,CAAC;EAEF,IAAMC,SAAS,GAAG,SAAZA,SAASA,CAAA,EAAkB;IAAA,IAAdC,KAAK,GAAAvB,SAAA,CAAAC,MAAA,QAAAD,SAAA,QAAAE,SAAA,GAAAF,SAAA,MAAG,CAAC;IAC1B,OAAOwB,MAAM,CAACC,IAAI,CAACC,MAAM,CAAC,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC,CAACC,SAAS,CAAC,CAAC,EAAEL,KAAK,GAAG,CAAC,CAAC;EACpE,CAAC;EACD,IAAMM,WAAW,WAAAC,MAAA,CAAWR,SAAS,CAAC,CAAC,OAAAQ,MAAA,CAAI,EAAEjC,SAAS,CAAE;EAExD,IAAIkC,aAAa,GAAG,IAAI;;EAExB;EACA,IAAMC,IAAI,GAAG,SAAPA,IAAIA,CAAA,EAAS;IACjBC,OAAO,CAACC,GAAG,CAACL,WAAW,EAAEM,QAAQ,CAACC,cAAc,CAACP,WAAW,CAAC,CAAC;IAC9DE,aAAa,GAAG3C,OAAO,CAAC4C,IAAI,CAC1BG,QAAQ,CAACC,cAAc,CAACP,WAAW,CAAC,EACpC,IAAI,EACJ;MACE;MACAQ,QAAQ,EAAE;IACZ,CACF,CAAC;IACDC,SAAS,CAACnC,WAAW,CAAC;EACxB,CAAC;
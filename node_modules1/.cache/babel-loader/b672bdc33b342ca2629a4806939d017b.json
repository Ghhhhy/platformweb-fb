m '../model/getEchartsConfig'\r\nimport { throttle } from '@/utils/throttle'\r\n\r\n/**\r\n * 图表\r\n * @param echarts\r\n * @param config\r\n * @param observeNode 需要监听的节点变化 resize图表\r\n * @returns {{setOption: setOption, getChartInstance: (function(): null), resize: (function(): void)}}\r\n */\r\nexport const useBarChart = (echarts, config, observeNode = null) => {\r\n  const { selecterId } = config\r\n  let chartInstance = null\r\n  const options = {\r\n    backgroundColor: getColor('white'),\r\n    color: getColor(['blue', 'green']),\r\n    title: getTitle(),\r\n    // 指示器\r\n    tooltip: {\r\n      ...getTooltip(),\r\n      formatter: getTooltipFormatter()\r\n    },\r\n    // 位置\r\n    grid: getGrid({ left: 16, top: 90, right: 16, bottom: 10 }),\r\n    // 图例\r\n    legend: getLegend(),\r\n    // x轴\r\n    xAxis: {\r\n      ...getAxis({ type: 'category', splitLineShow: true }),\r\n      axisLabel: getAxisLabel()\r\n    },\r\n    // y轴\r\n    yAxis: {\r\n      ...getAxis({ splitLineShow: true }),\r\n      axisLabel: getAxisLabel()\r\n    },\r\n    series: []\r\n  }\r\n\r\n  // 初始化\r\n  const init = () => {\r\n    chartInstance = echarts.init(document.getElementById(selecterId), null, {\r\n      // 渲染成svg 支持放大清晰度不变\r\n      renderer: 'svg'\r\n    })\r\n    chartInstance.setOption(options)\r\n  }\r\n\r\n  const getChartInstance = () => {\r\n    return chartInstance\r\n  }\r\n\r\n  const setOption = (option) => {\r\n    chartInstance?.setOption(option)\r\n  }\r\n\r\n  const resize = throttle(() => {\r\n    chartInstance?.resize()\r\n  }, 1000)\r\n\r\n  /**\r\n   * 监听节点style\r\n   */\r\n  const observeResize = () => {\r\n    // console.log(document.querySelector(observeNode))\r\n    const selecter = observeNode\r\n      ? document.querySelector(observeNode)\r\n      : document.querySelector('.nav')\r\n    if (!selecter) return\r\n    const observer = new MutationObserver(resize)\r\n    observer.observe(selecter, {\r\n      attributes: true,\r\n      attributeFilter: ['style']\r\n    })\r\n  }\r\n\r\n  onMounted(() => {\r\n    init()\r\n    observeResize()\r\n    setTimeout(() => {\r\n      chartInstance?.resize()\r\n    })\r\n  })\r\n\r\n  return {\r\n    getChartInstance,\r\n    setOption,\r\n    resize\r\n  }\r\n}\r\n"],"mappings":";;;;;;AAAA,SAASA,SAAS,QAAQ,sBAAsB;AAChD,SACEC,SAAS,EACTC,OAAO,EACPC,OAAO,EACPC,UAAU,EACVC,mBAAmB,EACnBC,QAAQ,EACRC,QAAQ,EACRC,YAAY,QACP,2BAA2B;AAClC,SAASC,QAAQ,QAAQ,kBAAkB;;AAE3C;AACA;AACA;AACA;AACA;AACA;AACA;AACA,OAAO,IAAMC,WAAW,GAAG,SAAdA,WAAWA,CAAIC,OAAO,EAAEC,MAAM,EAAyB;EAAA,IAAvBC,WAAW,GAAAC,SAAA,CAAAC,MAAA,QAAAD,SAAA,QAAAE,SAAA,GAAAF,SAAA,MAAG,IAAI;EAC7D,IAAQG,UAAU,GAAKL,MAAM,CAArBK,UAAU;EAClB,IAAIC,aAAa,GAAG,IAAI;EACxB,IAAMC,OAAO,GAAG;IACdC,eAAe,EAAEd,QAAQ,CAAC,OAAO,CAAC;IAClCe,KAAK,EAAEf,QAAQ,CAAC,CAAC,MAAM,EAAE,OAAO,CAAC,CAAC;IAClCgB,KAAK,EAAEf,QAAQ,CAAC,CAAC;IACjB;IACAgB,OAAO,EAAAC,aAAA,CAAAA,aAAA,KACFpB,UAAU,CAAC,CAAC;MACfqB,SAAS,EAAEpB,mBAAmB,CAAC;IAAC,EACjC;IACD;IACAqB,IAAI,EAAEvB,OAAO,CAAC;MAAEwB,IAAI,EAAE,EAAE;MAAEC,GAAG,EAAE,EAAE;MAAEC,KAAK,EAAE,EAAE;MAAEC,MAAM,EAAE;IAAG,CAAC,CAAC;IAC3D;IACAC,MAAM,EAAE9B,SAAS,CAAC,CAAC;IACnB;IACA+B,KAAK,EAAAR,aAAA,CAAAA,aAAA,KACAtB,OAAO,CAAC;MAAE+B,IAAI,EAAE,UAAU;MAAEC,aAAa,EAAE;IAAK,CAAC,CAAC;MACrDC,SAAS,EAAE3B,YAAY,CAAC;IAAC,EAC1B;IACD;IACA4B,KAAK,EAAAZ,aAAA,CAAAA,aAAA,KACAtB,OAAO,CAAC;MAAEgC,aAAa,EAAE;IAAK,CAAC,CAAC;MACnCC,SAAS,EAAE3B,YAAY,CAAC;IAAC,EAC1B;IACD6B,MAAM,EAAE;EACV,CAAC;;EAED;EACA,IAAMC,IAAI,GAAG,SAAPA,IAAIA,CAAA,EAAS;IACjBpB,aAAa,GAAGP,OAAO,CAAC2B,IAAI,CAACC,QAAQ,CAACC,cAAc,CAACvB,UAAU,CAAC,EAAE,IAAI,EAAE;MACtE;MACAwB,QAAQ,EAAE;IACZ,CAAC,CAAC;IACFvB,aAAa,CAACwB,SAAS,CAACvB,OAAO,CAAC;EAClC,CAAC;EAED,IAAMwB,gBAAgB,GAAG,SAAnBA,gBAAgBA,CAAA,EAAS;IAC7B,OAAOzB,aAAa;EACtB,CAAC;EAED,IAAMwB,SAAS,GAAG,SAAZA,SAASA,CAAIE,MAAM,EAAK;IAAA,IAAAC,cAAA;IAC5B,CAAAA,cAAA,GAAA3B,aAAa,cAAA2B,cAAA,uBAAbA,cAAA,CAAeH,SAAS,CAACE,MAAM,CAAC;EAClC,CAAC;EAED,IAAME,MAAM,GAAGrC,QAAQ,CAAC,YAAM;IAAA,IAAAsC,eAAA;IAC5B,CAAAA,eAAA,GAAA7B,aAAa,cAAA6B,eAAA,uBAAbA,eAAA,CAAeD
{
  "name": "@bszx/boss-ui",
  "version": "2.1.2-beta.19",
  "description": "feat: formmater自定义展示提示的时候，根据约定的溢出文字长度来显示",
  "author": "lilingli",
  "keywords": [
    "boss-ui",
    "bs-ui",
    "Bs-UI",
    "Boss-UI",
    "Bs-ui"
  ],
  "main": "lib/boss-ui.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev": "npm run serve",
    "build": "vue-cli-service build",
    "build:common": "webpack --config build/webpack.common.js",
    "lint": "eslint --fix --ext .js,.vue packages",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "async": "npm run clean && npm run build:common",
    "push": "yalc push",
    "commit": "git-cz",
    "pub": "sh ./autoDeploy/deploy.sh",
    "pub:reback": "sh ./autoDeploy/reback.sh",
    "genlog": "conventional-changelog -p angular -i .github/CHANGELOG.md -s -r 0 && git add .github/CHANGELOG.md"
  },
  "gitHooks": {
    "commit-msg": "node .github/verify-commit-msg.js"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@bszx/bs-vxe-table": "^2.10.2-0.3",
    "async-validator": "^1.11.5",
    "element-ui": "2.13.2",
    "file-saver": "^2.0.5",
    "jsoneditor": "^9.7.2",
    "jszip": "^3.5.0",
    "lodash": "^4.17.21",
    "sortablejs": "^1.14.0",
    "vue": "2.6.10",
    "vue-aplayer": "^1.6.1",
    "vue-splitpane": "^1.0.6",
    "vue-template-compiler": "2.6.10",
    "vue-virtual-scroller": "1.0.10",
    "vxe-table": "2.10.4",
    "vxe-table-plugin-element": "1.8.10",
    "vxe-table-plugin-excel": "1.4.1",
    "vxe-table-plugin-export-xlsx": "1.6.0",
    "vxe-table-plugin-menus": "1.4.0",
    "xe-utils": "2.8.3",
    "xlsx": "0.16.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "7.13.10",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "autoprefixer": "^8",
    "babel-plugin-module-resolver": "^3.1.1",
    "chalk": "2.3.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.2.2",
    "core-js": "^3.6.4",
    "cross-env": "^3.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "js-base64": "^3.7.2",
    "lint-staged": "^12.3.4",
    "node-sass": "4.14.1",
    "npm-cli-login": "^1.0.0",
    "prettier": "^2.5.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.0",
    "shelljs": "^0.8.5",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "vue-loader": "^15.9.6",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^3.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
